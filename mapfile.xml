<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE Map[]>
<Map srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs" background-color="#bbdefb">

<Style name="ne10mland" filter-mode="first">
  <Rule>
    <PolygonSymbolizer fill-opacity="1" fill="#e3f2fd" />
  </Rule>
</Style>
<Layer name="ne10mland"
  srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">
    <StyleName>ne10mland</StyleName>
    <Datasource>
       <Parameter name="file"><![CDATA[./world/layers/ne10mland/ne_10m_land.shp]]></Parameter>
       <Parameter name="type"><![CDATA[shape]]></Parameter>
    </Datasource>
  </Layer>

<Style name="ne10mlakes" filter-mode="first">
  <Rule>
    <PolygonSymbolizer fill-opacity="1" fill="#bbdefb" />
  </Rule>
</Style>
<Layer name="ne10mlakes"
  srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">
    <StyleName>ne10mlakes</StyleName>
    <Datasource>
       <Parameter name="file"><![CDATA[./world/layers/ne10mlakes/ne_10m_lakes.shp]]></Parameter>
       <Parameter name="type"><![CDATA[shape]]></Parameter>
    </Datasource>
  </Layer>

<Style name="ne10mglaciatedareas" filter-mode="first">
  <Rule>
    <PolygonSymbolizer fill-opacity="1" fill="#fafafa" />
  </Rule>
</Style>
<Layer name="ne10mglaciatedareas"
  srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">
    <StyleName>ne10mglaciatedareas</StyleName>
    <Datasource>
       <Parameter name="file"><![CDATA[./world/layers/ne10mglaciatedareas/ne_10m_glaciated_areas.shp]]></Parameter>
       <Parameter name="type"><![CDATA[shape]]></Parameter>
    </Datasource>
  </Layer>

</Map>
